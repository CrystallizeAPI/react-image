{
  "name": "@crystallize/react-image",
  "description": "A React package to output an img tag with different source variations from Crystallize",
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:snowballdigital/react-image.git"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "parcel-bundler": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "scripts": {
    "dev": "NODE_ENV=development parcel ./public/index.html",
    "build": "webpack --mode=development",
    "prepare": "npm run build"
  },
  "main": "build/index.js",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-react-jsx"
    ]
  }
}
