{
    "name": "@crystallize/react-image",
    "description": "A React package to output an image with different source variations from Crystallize",
    "version": "3.1.0",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:CrystallizeAPI/react-image.git"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.1",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "@babel/register": "^7.5.5",
        "parcel-bundler": "^1.12.3",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "rollup": "^1.17.0",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.0.1",
        "rollup-plugin-node-resolve": "^5.2.0"
    },
    "peerDependencies": {
        "prop-types": ">=15.0.0"
    },
    "scripts": {
        "dev": "NODE_ENV=development parcel ./public/index.html",
        "build:rollup": "rollup -c --sourcemap",
        "build": "babel src/index.js --out-file build/index.js --source-maps",
        "prepare": "npm run build"
    },
    "main": "build/index.js"
}